{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
    "caseSensitive": false,
    "import": [
        "@cspell/dict-public-licenses/cspell-ext.json",
        "@cspell/dict-scientific-terms-us/cspell-ext.json",
        "@cspell/dict-companies/cspell-ext.json",
        "@cspell/dict-docker/cspell-ext.json",
        "@cspell/dict-k8s/cspell-ext.json",
        "@cspell/dict-java/cspell-ext.json",
        "@cspell/dict-typescript/cspell-ext.json",
        "@cspell/dict-filetypes/cspell-ext.json",
        "@cspell/dict-git/cspell-ext.json",
        "@cspell/dict-fullstack/cspell-ext.json",
        "@cspell/dict-software-terms/cspell-ext.json",
        "@cspell/dict-html-symbol-entities/cspell-ext.json",
        "@cspell/dict-npm/cspell-ext.json",
        "@cspell/dict-fr-fr/cspell-ext.json",
        "@cspell/dict-medicalterms/cspell-ext.json",
        "@cspell/dict-cryptocurrencies/cspell-ext.json",
        "@cspell/dict-fonts/cspell-ext.json",
        "@cspell/dict-cpp/cspell-ext.json",
        "@cspell/dict-node/cspell-ext.json",
        "@cspell/dict-html/cspell-ext.json",
        "@cspell/dict-css/cspell-ext.json",
        "@cspell/dict-russian/cspell-ext.json",
        "@cspell/dict-bash/cspell-ext.json",
        "@cspell/dict-en-gb/cspell-ext.json",
        "@cspell/dict-gaming-terms/cspell-ext.json"
    ],
    "patterns": [
        {
            "name": "ignoreTuiPrefix",
            "pattern": "/\\b(tui[A-Za-z0-9_]*|tui-[a-z0-9-]+)/g"
        },
        {
            "name": "ignoreNgPrefix",
            "pattern": "/\\b(ng[A-Za-z0-9_]*|ng-[a-z0-9-]+)/g"
        },
        {
            "name": "ignoreAria",
            "pattern": "/\\baria-[a-z0-9-]+/g"
        },
        {
            "name": "ignoreData",
            "pattern": "/\\bdata-[a-z0-9-]+/g"
        },
        {
            "name": "ignoreOnEvents",
            "pattern": "/\\bon[a-z]+/g"
        },
        {
            "name": "ignoreHtmlAttributes",
            "pattern": "/\\b(controlsList|autocomplete|autofocus|tabindex|spellcheck|contenteditable|maxlength|inputmode|crossorigin|srcset)/g"
        },
        {
            "name": "ignoreKebabHtml",
            "pattern": "/\\b[a-z]+(?:-[a-z0-9]+)+/g"
        },
        {
            "name": "ignoreJsCamelTech",
            "pattern": "/\\b(?:transitionstart|transitionend|compositionstart|compositionend|dragenter|dragleave|dragover|dragend|touchstart|touchmove|touchend|scrollend|mouseenter|mouseleave|mousemove|mousedown|mouseup|keydown|keyup|keypress)/g"
        },
        {
            "name": "ignorePrePost",
            "pattern": "/\\b(pre|post)[A-Za-z0-9_-]+/g"
        },
        {
            "name": "ignoreTechCompositeWords",
            "pattern": "/(\\b([A-Z][a-z0-9]+)+\\b|\\b[a-z]+(?:[A-Z][a-z0-9]+)+\\b|\\b[a-z0-9]+(?:[a-z0-9]+){2,}\\b)/g"
        },
        {
            "name": "ignoreMarkdownCodeBlock",
            "pattern": "/```[\\s\\S]*?```/g"
        },
        {
            "name": "ignoreMarkdownInlineCode",
            "pattern": "/`[^`]+`/g"
        },
        {
            "name": "ignoreMustacheDouble",
            "pattern": "/\\{\\{[^{}]*\\}\\}/g"
        },
        {
            "name": "ignoreMustacheBlock",
            "pattern": "/\\{#[\\s\\S]*?#\\}/g"
        },
        {
            "name": "ignoreTwigBlock",
            "pattern": "/\\{%[\\s\\S]*?%\\}/g"
        },
        {
            "name": "ignoreHtmlTags",
            "pattern": "/<[^>]+>/g"
        },
        {
            "name": "ignoreJsCodeInsideComments",
            "pattern": "/(?<=\\/\\/.*?)([A-Za-z_][A-Za-z0-9_]*(?:\\.[A-Za-z0-9_]+)*\\(?.*?\\)?)/g"
        },
        {
            "name": "ignoreImports",
            "pattern": "/\\bimport\\s+[^;]+;/g"
        },
        {
            "name": "ignoreTypeReferences",
            "pattern": "/:\\s*[A-Za-z_][A-Za-z0-9_\\.<>\\[\\]]+/g"
        }
    ],
    "ignoreRegExpList": [
        "ignoreTuiPrefix",
        "ignoreNgPrefix",
        "ignoreAria",
        "ignoreData",
        "ignoreOnEvents",
        "ignoreHtmlAttributes",
        "ignoreKebabHtml",
        "ignoreJsCamelTech",
        "ignorePrePost",
        "ignoreTechCompositeWords",
        "ignoreMarkdownCodeBlock",
        "ignoreMarkdownInlineCode",
        "ignoreMustacheDouble",
        "ignoreMustacheBlock",
        "ignoreTwigBlock",
        "ignoreHtmlTags",
        "ignoreJsCodeInsideComments",
        "ignoreImports",
        "ignoreTypeReferences"
    ],
    "words": [
        "benji",
        "cstm",
        "cvcpo",
        "khtml",
        "laquo",
        "maskito",
        "metrika",
        "polymorpheus",
        "raquo",
        "tbank",
        "utdl",
        "checkmark",
        "fullsize",
        "recalc",
        "бивер",
        "фейд",
        "шиммер",
        "codeowners",
        "tseslint",
        "tsestree",
        "санитайзер",
        "hilite"
    ]
}
